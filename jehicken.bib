    doi = {10.1007/s00158-014-1183-y},
    keywords = {optimization, rom},
    number = {4},
    pages = {919--940},
    posted-at = {2015-07-08 19:08:08},
    priority = {3},
    publisher = {Springer Berlin Heidelberg},

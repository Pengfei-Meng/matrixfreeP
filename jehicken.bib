    issn = {0951-7715},
    journal = {Nonlinearity},
    keywords = {chaotic, optimization},
    month = sep,
    number = {5},
    pages = {1867--1889},
    posted-at = {2013-09-07 16:47:12},
    priority = {0},
    title = {{Ruelle's linear response formula, ensemble adjoint schemes and L\'{e}vy flights}},

    journal = {Int. J. Numer. Meth. Fluids},
    keywords = {uq},
    month = mar,
    number = {8-9},
    pages = {773--788},
    posted-at = {2014-07-03 14:29:52},
    priority = {2},
    publisher = {John Wiley \& Sons, Ltd.},
    title = {{Certified real-time solution of the parametrized steady incompressible Navier–Stokes equations: rigorous reduced-basis a posteriori error bounds}},

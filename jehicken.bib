    posted-at = {2015-02-08 02:03:53},
    priority = {0},
    publisher = {Springer Netherlands},
    title = {{A diagonal-mass-matrix triangular-spectral-element method based on cubature points}},
    url = {http://dx.doi.org/10.1007/s10665-006-9085-7},
    volume = {56},
    year = {2006}
}

